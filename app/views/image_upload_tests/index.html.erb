<p id="notice"><%= notice %></p>

<h1>Image Upload Tests</h1>

<table>
  <thead>
    <tr>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @image_upload_tests.each do |image_upload_test| %>
      <tr>
        <td><%= image_upload_test.image %></td>
        <td><%= link_to 'Show', image_upload_test %></td>
        <td><%= link_to 'Edit', edit_image_upload_test_path(image_upload_test) %></td>
        <td><%= link_to 'Destroy', image_upload_test, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Image Upload Test', new_image_upload_test_path %>
